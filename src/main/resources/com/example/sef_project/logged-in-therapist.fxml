            <TableView fx:id="t_today" layoutX="1.0" layoutY="87.0" prefHeight="200.0" prefWidth="312.0" visible="true">
              <columns>
                <TableColumn fx:id="t_start" prefWidth="98.0" text="start_time" />
                 <TableColumn fx:id="t_end" prefWidth="75.0" text="end_time" />
                <TableColumn fx:id="t_patient" prefWidth="138.0" text="patient" />
              </columns>
            </TableView>
            <Button fx:id="b_today_app" layoutX="65.0" layoutY="25.0" mnemonicParsing="false" text="Show today appointments" />
