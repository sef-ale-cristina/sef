               </columns>
            </TableView>
            <Button fx:id="b_history" layoutX="105.0" layoutY="22.0" mnemonicParsing="false" text="Show history" />
         </children>
      </AnchorPane>

      <Button fx:id="b_logout" layoutX="684.0" layoutY="29.0" mnemonicParsing="false" text="Log Out" />
   </children>
</AnchorPane>
