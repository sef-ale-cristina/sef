<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #CEF5D4;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sef_project.LoggedInTherapist">
   <children>
      <AnchorPane layoutX="67.0" layoutY="80.0" prefHeight="237.0" prefWidth="251.0">
